<div class="row">
<div class="col-md-12">
<h2>Demos</h2>
<hr />

<div class="table-responsive">
            <table class="table ">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Title</th>
                        <th>Post Date</th>
                        <th>Category</th>
                        <th>Published</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each products}}
                    <tr>
                        <td>{{this.id}}</td>
                        <td>{{this.title}}</td>
                        <td>{{this.postDate}}</td>
                        <td><a href="/demos?category={{this.category}}">{{this.category}}</a></td>
                        <td>{{this.published}}</td>
                    </tr>
                    {{/each}}

                </tbody>
            </table>
        </div>

    </div>
    
    {{#unless demos}}
        <div class="col-md-12 text-center">
            <strong>{{message}}</strong>
        </div>
  {{/unless}}
</div>
